{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Debug (MinGW)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/Strings/reversing-string-method1.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/Strings",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++.exe build active file"
    }
  ]
}
